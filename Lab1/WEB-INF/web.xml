<?xml version="1.0" encoding="UTF8"?>
<web-app xmlns="http://java.sun.com/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchemainstance" xsi:schemaLocation="http://java.sun.com/xml/ns/javaee http://java.sun.com/xml/ns/javaee/webapp_3_0.xsd" version="3.0">
  <servlet>
   <servlet-name>
    WebController
   </servlet-name>
   <servlet-class>
     controller.WebController
   </servlet-class>
  </servlet>

  <servlet-mapping>
   <servlet-name>
    WebController
   </servlet-name>
   <url-pattern>
    /login
   </url-pattern>
   <url-pattern>
    /add
   </url-pattern>
   <url-pattern>
    /table
   </url-pattern>
   <url-pattern>
    /exit
   </url-pattern>
   <url-pattern>
    /delete
   </url-pattern>
   <url-pattern>
    /register
   </url-pattern>
   <url-pattern>
    /UserCreate
   </url-pattern>
  </servlet-mapping>
</web-app>